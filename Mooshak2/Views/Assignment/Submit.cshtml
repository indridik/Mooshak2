@model Mooshak2.Models.Entities.Assignment
@{
    ViewBag.Title = "C++ Compile test page";
}
<div class="page-header">
    <h1>@Model.Title</h1>
</div>
<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("Submit", "Assignment", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input type="file" name="file" required="required" />
            <input type="Submit" class="btn btn-primary" value="Compile Test Code" />
        }

        @if (ViewBag.Output != null)
        {
            <ul>
                @foreach (var line in ViewBag.Output)
                {
                    <li>@line</li>
                }
            </ul>}
    </div>
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
    </div>
</div>
